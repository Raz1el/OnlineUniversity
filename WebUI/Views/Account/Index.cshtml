@model IEnumerable<WebUI.Models.ApplicationUser>

@{
    ViewBag.Title = "Пользователи";
}

<h2>Пользователи</h2>
<table class="table">
    <tr>
        <th>Имя</th>
        <th>Группа</th>
        <th>Электронный адрес</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Group</td>
            <td>@item.Email</td>
            <td>
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }
</table>